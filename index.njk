<script>
(function() {
  const revenueInput = document.getElementById('revenue');
  const feeSelect = document.getElementById('currentFee');
  const currentLoss = document.getElementById('currentLoss');
  const newCost = document.getElementById('newCost');
  const annualSavings = document.getElementById('annualSavings');
  
  function calculate() {
    const revenue = parseFloat(revenueInput.value.replace(/[^0-9.]/g, '')) || 0;
    const feePercent = parseFloat(feeSelect.value) || 0;
    
    const monthlyLoss = revenue * (feePercent / 100);
    const monthlyNew = revenue * 0.01;
    const yearlySavings = (monthlyLoss - monthlyNew) * 12;
    
    currentLoss.textContent = '$' + Math.round(monthlyLoss).toLocaleString() + '/mo';
    newCost.textContent = '$' + Math.round(monthlyNew).toLocaleString() + '/mo';
    annualSavings.textContent = '$' + Math.round(yearlySavings).toLocaleString() + '/year';
  }
  
  revenueInput.addEventListener('input', calculate);
  feeSelect.addEventListener('change', calculate);
  calculate();
})();
</script>

